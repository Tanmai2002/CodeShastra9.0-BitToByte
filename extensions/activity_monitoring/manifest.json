{
    "name": "My Extension",
    "description": "Logs the URL of visited websites to a Flask app",
    "version": "1.0",
    "manifest_version": 2,
    "background": {
      "scripts": ["background.js"],
      "persistent": false
    },
    "permissions": ["tabs", "http://127.0.0.1:5000*"]
  }
  